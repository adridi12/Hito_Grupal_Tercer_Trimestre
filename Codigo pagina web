                </select>
                <input type="text" id="value">
                </fieldset>
            </form>
            <section id="sect">
        </section>


            <script type="text/javascript">

                function borrar()
                {
                info.innerHTML = "";
                sect.innerHTML = "";
                }

                function todo(){
                var valor = "";
                function update(valor) { 
                var select = document.getElementById('nombre');
                var option = select.options[select.selectedIndex];
                document.getElementById('value').value = option.value;
                var valor = option.value;
                return valor;
                }
                valor = update(valor);
                fetch("json.json")
                    .then(response => response.json())
                    .then(data => {
                        console.log(data);
                        update();
                        console.log("Valor fetch: " + valor);
                        valor = update(valor);
                        populateDiv(data);
                        populatePre(data);
                    });


                    function populateDiv(obj) {
                        const section = document.querySelector('#info');
                        const myInfo = document.createElement('h1');
                        myInfo.textContent = `RESULTADO`;
                        section.appendChild(myInfo);
                    }


                    function populatePre(obj) {
                        const section2 = document.querySelector('#sect');
                        const alumnos = obj.alumnos;


                        for (const alum of alumnos) {
                            if(alum.name == valor){
                                const myArticle = document.createElement('article');
                                const myH2 = document.createElement('h2');
                                const myPara1 = document.createElement('p');
                                const myPara2 = document.createElement('p');
                                const myPara3 = document.createElement('p');
                                const myUl = document.createElement('ul');
                                const myH3Ul = document.createElement('h3') ;
                                const myLi = document.createElement('li') ;
                                const myLia = document.createElement('li') ;
                                const myLib = document.createElement('li') ;
                                const myLic = document.createElement('li') ;
                                const myLid = document.createElement('li') ;
                                const myLie = document.createElement('li') ;
                                const myLif = document.createElement('li') ;
                                const myLig = document.createElement('li') ;
                                const myLih = document.createElement('li') ;
                                const myLii = document.createElement('li') ;
                                
                                myH2.textContent = alum.name;
                                myPara1.textContent = `Encuesta realizada:  ${alum.Marca_temporal}`;
                                myPara2.textContent = `Edad:  ${alum.Edad}`;
                                myPara3.textContent = `Centro:  ${alum.Centro_de_Campus_en_el_que_estudias}`;
                                myPara3.textContent = `¬øQu√© estudias?:  ${alum.Grado_en_el_que_estudias}`;
                                myH3Ul.textContent = `TODAS LAS PREGUNTAS:`;

                                
                                myUl.appendChild(myH3Ul);

                                
                                myLia.textContent = `¬øCu√°nto tiempo pasas al movil?:  ${alum.Cuanto_tiempo_estas_con_el_movil}`;
                                myLib.textContent = `¬øG√©nero?: ${alum.Sexo}`;
                                myLic.textContent = `¬øCu√°ntas apps tienes instaladas(RRSS)?:  ${alum.Cuantas_aplicaciones_del_tipo_red_social_tienes_instaladas}`;
                                myLid.textContent = `¬øPara qu√© usas el movil?:  ${alum.Para_que_usas_el_movil}`;
                                myLie.textContent = `¬øQu√© app usas para escuchar m√∫sica?:  ${alum.Que_aplicaci√≥n_usas_con_mayor_frecuencia_para_escuchar_musica}`;
                                myLif.textContent = `¬øQu√© app usas para ver series?:  ${alum.Que_aplicaci√≥n_usas_con_mayor_frecuencia_para_ver_series}`;
                                myLig.textContent = `¬øPor donde te enteras de las noticias?:  ${alum.Te_enteras_de_las_noticias_antes_en}`;
                                myLih.textContent = `¬øC√≥mo te entretienes?:  ${alum.Prefieres_entretenerte}`;
                                myLii.textContent = `¬øPodr√≠as vivir sin el tel√©fono?:  ${alum.Podrias_estar_24h_sin_usar_el_telefonomovil}`;
                                
                                
                                myUl.appendChild(myLia);
                                myUl.appendChild(myLib);
                                myUl.appendChild(myLic);
                                myUl.appendChild(myLid);
                                myUl.appendChild(myLie);
                                myUl.appendChild(myLif);
                                myUl.appendChild(myLig);
                                myUl.appendChild(myLih);
                                myLih.appendChild(myLii);

                                myArticle.appendChild(myH2);
                                myArticle.appendChild(myPara1);
                                myArticle.appendChild(myPara2);
                                myArticle.appendChild(myPara3);
                                myArticle.appendChild(myUl);

                                section2.appendChild(myArticle);
                            }
                        }
                       
                    }

                
                function populate() {
                    fetch("json.json")
                        .then(response => response.json())
                        .then(data => {
                            console.log(data);
                            update(valor);
                            populateDiv(data);
                            populatePre(data);
                });
                }
            }
            
                
